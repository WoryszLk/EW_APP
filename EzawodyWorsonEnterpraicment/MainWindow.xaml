        <!--//window Border //-->
        <Border Background="#222831" CornerRadius="20">
        </Border>

        <Grid>
            <!--//  Base Grid //-->
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="225"/>
                <ColumnDefinition Width="625"/>
            </Grid.ColumnDefinitions>
            
        </Grid>
    </Grid>
</Window>
